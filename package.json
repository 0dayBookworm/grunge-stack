{
  "private": true,
  "name": "remix-app-template",
  "description": "",
  "license": "",
  "scripts": {
    "postinstall": "remix setup node",
    "build": "remix build",
    "dev:remix": "remix watch",
    "dev:arc": "cross-env ARC_LOCAL=1 ARC_TABLES_PORT=5555 node --require ./mocks ./node_modules/.bin/arc sandbox",
    "dev": "run-p dev:*",
    "format": "prettier --write .",
    "lint": "eslint --cache-location ./node_modules/.cache/eslint .",
    "typecheck": "tsc -b && tsc -b cypress",
    "test": "vitest",
    "test:e2e:dev": "cross-env ENABLE_TEST_ROUTES=true start-server-and-test dev http://localhost:3333 'cypress open'",
    "test:e2e:run": "cross-env PORT=8811 ENABLE_TEST_ROUTES=true start-server-and-test dev http://localhost:8811 'cypress run'"
  },
  "dependencies": {
    "@architect/functions": "^4.1.2",
    "@reach/alert": "^0.16.0",
    "@remix-run/architect": "*",
    "@remix-run/react": "*",
    "bcryptjs": "2.4.3",
    "cuid": "^2.1.8",
    "react-dom": "^17.0.2",
    "react": "^17.0.2",
    "remix": "*",
    "tiny-invariant": "^1.2.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.7",
    "@remix-run/dev": "*",
    "@remix-run/eslint-config": "*",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/architect__functions": "^3.13.6",
    "@types/bcryptjs": "2.4.2",
    "@types/eslint": "^8.4.1",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@vitejs/plugin-react": "^1.2.0",
    "c8": "^7.11.0",
    "cross-env": "^7.0.3",
    "cypress": "^9.5.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.23",
    "esbuild-register": "^3.3.2",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-testing-library": "^5.0.5",
    "happy-dom": "^2.41.0",
    "mock-aws-s3": "^4.0.2",
    "msw": "^0.38.1",
    "nock": "^13.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.5.1",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.5.5",
    "vite-tsconfig-paths": "^3.4.1",
    "vitest": "^0.5.5"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false,
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/server/build",
    "/public/build"
  ]
}
